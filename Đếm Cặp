#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    long long N,X;
    cin>>N>>X;
    int a[N];
    int count = 0;
    for (int m = 0; m < N;m++)
    {
        cin>> a[m];
    }
    for (int i = 0; i < N; i++) 
    {
        for (int j = i; j < N; j++) 
        {
            if (pow(a[i], 2) + a[j] == X) {
                count++;
            }
        }
    }
    cout << count;
    return 0;
}
